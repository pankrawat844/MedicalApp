<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>

        <variable
            name="add"
            type="com.app.medicalapp.activity.auth.signup.doc.docDetails.clinicDetails.AddClinicActivity" />
    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/backgroundColor"
        tools:context=".activity.auth.signup.doc.docDetails.clinicDetails.AddClinicActivity">

        <include
            android:id="@+id/include"
            layout="@layout/custom_toolbar"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <EditText
            android:id="@+id/clinic_name"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="16dp"
            android:layout_marginTop="16dp"
            android:layout_marginRight="10dp"
            android:background="#00000000"
            android:fontFamily="@font/open_sans_regular"
            android:hint="CLINIC NAME"
            android:textColor="@color/white"
            android:textColorHint="#80ffffff"
            app:layout_constraintEnd_toStartOf="@+id/imageView4"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/include" />

        <View
            android:id="@+id/view9"
            android:layout_width="0dp"
            android:layout_height="2dp"
            android:layout_marginTop="9dp"
            android:background="#E6ffffff"
            app:layout_constraintEnd_toEndOf="@+id/clinic_name"
            app:layout_constraintStart_toStartOf="@+id/clinic_name"
            app:layout_constraintTop_toBottomOf="@+id/clinic_name" />
        <ImageView
            android:id="@+id/imageView4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:layout_marginEnd="16dp"
            android:layout_marginRight="16dp"
            android:src="@drawable/ic_launcher_background"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/include" />

        <TextView
            android:id="@+id/add_clinic"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:layout_marginEnd="16dp"
            android:layout_marginRight="16dp"
            android:background="#00000000"
            android:drawableRight="@drawable/ic_keyboard_arrow_right_black_24dp"
            android:fontFamily="@font/open_sans_regular"
            android:hint="ADD CLINIC ADDRESS"
            android:textColor="@color/white"
            android:layout_marginStart="16dp"
            android:layout_marginLeft="16dp"
            android:textColorHint="#80ffffff"
            android:textSize="18dp"
            android:onClick="@{add::addAddress}"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageView4" />

        <View
            android:id="@+id/view11"
            android:layout_width="match_parent"
            android:layout_height="2dp"
            android:layout_marginStart="16dp"
            android:layout_marginLeft="16dp"
            android:layout_marginTop="9dp"
            android:layout_marginEnd="16dp"
            android:layout_marginRight="16dp"
            android:background="#E6ffffff"
            app:layout_constraintEnd_toEndOf="@+id/add_clinic"
            app:layout_constraintStart_toStartOf="@+id/add_clinic"
            app:layout_constraintTop_toBottomOf="@+id/add_clinic" />

        <EditText
            android:id="@+id/add_clinic_timming"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:background="#00000000"
            android:drawableRight="@drawable/ic_keyboard_arrow_right_black_24dp"
            android:fontFamily="@font/open_sans_regular"
            android:hint="ADD CLINIC TIMMING"
            android:textColor="@color/white"
            android:textColorHint="#80ffffff"
            app:layout_constraintEnd_toEndOf="@+id/view11"
            app:layout_constraintStart_toStartOf="@+id/view11"
            app:layout_constraintTop_toBottomOf="@+id/view11" />

        <View
            android:id="@+id/view12"
            android:layout_width="match_parent"
            android:layout_height="2dp"
            android:layout_marginStart="16dp"
            android:layout_marginLeft="16dp"
            android:layout_marginTop="9dp"
            android:layout_marginEnd="16dp"
            android:layout_marginRight="16dp"
            android:background="#E6ffffff"
            app:layout_constraintEnd_toEndOf="@+id/add_clinic_timming"
            app:layout_constraintStart_toStartOf="@+id/add_clinic_timming"
            app:layout_constraintTop_toBottomOf="@+id/add_clinic_timming" />
        <!--<Button
            android:id="@+id/button10"
            android:layout_width="200dp"
            android:layout_height="60dp"
            android:layout_marginTop="64dp"
            android:background="@drawable/rect_white_with_border"
            android:text="Save"
            android:textColor="@color/design_dark_default_color_secondary"
            app:backgroundTint="@color/design_dark_default_color_primary_variant"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView18" />-->

    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>