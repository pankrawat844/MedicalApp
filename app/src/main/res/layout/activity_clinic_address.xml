<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>

    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/backgroundColor"
        tools:context=".activity.auth.signup.diagostic.centerDetails.AddressActivity">

        <include
            android:id="@+id/include2"
            layout="@layout/custom_toolbar"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <EditText
            android:id="@+id/state"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginLeft="16dp"
            android:layout_marginEnd="16dp"
            android:layout_marginRight="16dp"
            android:drawableRight="@drawable/ic_keyboard_arrow_right_black_24dp"
            android:hint="STATE"
            android:layout_marginTop="38.8dp"
            android:textSize="16sp"
            android:background="#00000000"
            android:fontFamily="@font/open_sans_regular"
            android:textColor="@color/white"
            android:textColorHint="#80ffffff"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/include2" />
        <View
            android:id="@+id/view5"
            android:layout_width="match_parent"
            android:layout_height="2dp"
            android:layout_marginStart="16dp"
            android:layout_marginLeft="16dp"
            android:layout_marginTop="9dp"
            android:layout_marginEnd="16dp"
            android:layout_marginRight="16dp"
            android:background="#E6ffffff"
            app:layout_constraintEnd_toEndOf="@+id/state"
            app:layout_constraintStart_toStartOf="@+id/state"
            app:layout_constraintTop_toBottomOf="@+id/state" />

        <EditText
            android:id="@+id/city"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:drawableRight="@drawable/ic_keyboard_arrow_right_black_24dp"
            android:hint="CITY"
            android:background="#00000000"
            android:fontFamily="@font/open_sans_regular"
            android:textColor="@color/white"
            android:textColorHint="#80ffffff"
            app:layout_constraintEnd_toEndOf="@+id/view5"
            app:layout_constraintStart_toStartOf="@+id/view5"
            app:layout_constraintTop_toBottomOf="@+id/view5" />

        <View
            android:id="@+id/view6"
            android:layout_width="match_parent"
            android:layout_height="2dp"
            android:layout_marginStart="16dp"
            android:layout_marginLeft="16dp"
            android:layout_marginTop="9dp"
            android:layout_marginEnd="16dp"
            android:layout_marginRight="16dp"
            android:background="#E6ffffff"
            app:layout_constraintEnd_toEndOf="@+id/city"
            app:layout_constraintStart_toStartOf="@+id/city"
            app:layout_constraintTop_toBottomOf="@+id/city" />

        <EditText
            android:id="@+id/area"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:drawableRight="@drawable/ic_keyboard_arrow_right_black_24dp"
            android:background="#00000000"
            android:fontFamily="@font/open_sans_regular"
            android:hint="AERA"
            android:textColor="@color/white"
            android:textColorHint="#80ffffff"
            app:layout_constraintEnd_toEndOf="@+id/view6"
            app:layout_constraintStart_toStartOf="@+id/view6"
            app:layout_constraintTop_toBottomOf="@+id/view6" />

        <View
            android:id="@+id/view7"
            android:layout_width="match_parent"
            android:layout_height="2dp"
            android:layout_marginStart="16dp"
            android:layout_marginLeft="16dp"
            android:layout_marginTop="9dp"
            android:layout_marginEnd="16dp"
            android:layout_marginRight="16dp"
            android:background="#E6ffffff"
            app:layout_constraintEnd_toEndOf="@+id/area"
            app:layout_constraintStart_toStartOf="@+id/area"
            app:layout_constraintTop_toBottomOf="@+id/area" />

        <EditText
            android:id="@+id/street"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:drawableRight="@drawable/ic_keyboard_arrow_right_black_24dp"
            android:hint="STREET"
            android:background="#00000000"
            android:fontFamily="@font/open_sans_regular"
            android:textColor="@color/white"
            android:textColorHint="#80ffffff"
            app:layout_constraintEnd_toEndOf="@+id/view7"
            app:layout_constraintStart_toStartOf="@+id/view7"
            app:layout_constraintTop_toBottomOf="@+id/view7" />

        <View
            android:id="@+id/view8"
            android:layout_width="match_parent"
            android:layout_height="2dp"
            android:layout_marginStart="16dp"
            android:layout_marginLeft="16dp"
            android:layout_marginTop="9dp"
            android:layout_marginEnd="16dp"
            android:layout_marginRight="16dp"
            android:background="#E6ffffff"
            app:layout_constraintEnd_toEndOf="@+id/street"
            app:layout_constraintStart_toStartOf="@+id/street"
            app:layout_constraintTop_toBottomOf="@+id/street" />

        <TextView
            android:id="@+id/google_map"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:drawableRight="@drawable/ic_keyboard_arrow_right_black_24dp"
            android:background="#00000000"
            android:fontFamily="@font/open_sans_regular"
            android:hint="GOOGlE MAP LOCATION"
            android:textColor="@color/white"
            android:textColorHint="#80ffffff"
            android:textSize="18dp"
            app:layout_constraintEnd_toEndOf="@+id/view8"
            app:layout_constraintStart_toStartOf="@+id/view8"
            app:layout_constraintTop_toBottomOf="@+id/view8" />

        <View
            android:id="@+id/view9"
            android:layout_width="match_parent"
            android:layout_height="2dp"
            android:layout_marginStart="16dp"
            android:layout_marginLeft="16dp"
            android:layout_marginTop="9dp"
            android:layout_marginEnd="16dp"
            android:layout_marginRight="16dp"
            android:background="#E6ffffff"
            app:layout_constraintEnd_toEndOf="@+id/google_map"
            app:layout_constraintStart_toStartOf="@+id/google_map"
            app:layout_constraintTop_toBottomOf="@+id/google_map" />


        <!-- <Button
             android:id="@+id/button9"
             android:layout_width="200dp"
             android:layout_height="60dp"
             android:layout_marginTop="64dp"
             android:background="@drawable/rect_white_with_border"
             android:text="Save"
             android:textColor="@color/design_dark_default_color_secondary"
             app:backgroundTint="@color/design_dark_default_color_primary_variant"
             app:layout_constraintEnd_toEndOf="parent"
             app:layout_constraintStart_toStartOf="parent"
             app:layout_constraintTop_toBottomOf="@+id/view9" />-->

    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>